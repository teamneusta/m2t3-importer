<phpunit colors="true">
    <testsuites>
        <testsuite name="m2t3_importer">
            <directory>Tests/Unit/</directory>
        </testsuite>
    </testsuites>
    <filter>
        <blacklist>
            <directory>Tests/</directory>
        </blacklist>
        <whitelist processUncoveredFilesFromWhitelist="false" addUncoveredFilesFromWhitelist="false">
            <directory>Classes/</directory>
        </whitelist>
    </filter>
</phpunit>
